<%
  @rules.each do |net, vals|
    unless vals.is_a?(Array)
      vals = [vals]
    end
    vals.each do |rule|
      rule.delete('protocol')
      rt = Hash['pref' => net].merge(rule)
-%>
<%= rt.flatten.join(' ') %>
<%
    end
  end
-%>
